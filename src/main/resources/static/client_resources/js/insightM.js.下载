function insightPMonitor() {
	try {	
	var nag = $(".CurrChnlCls");
	var size = nag.size();
	var webZone = "";
	var easyRem = "";
	var zone2 = "";
	var zone3 = "";
	var product = "";
	var events = "";
	var nameArray = [ "境内自驾游", ";e-神州自驾游保险", "境内自助游", ";e-神州逍遥游保险", "境内商务出行",
